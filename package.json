{
	"name": "lib-file-corrector",
	"displayName": "lib-file-corrector",
    "publisher": "ollyhayes",
	"description": "",
	"version": "0.0.3",
  "repository": {
    "type": "git",
    "url": "https://github.com/ollyhayes/lib-file-corrector.git"
  },
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"contributes": {
		"commands": [
			{
				"command": "lib-file-corrector.disableOnce",
				"title": "Disable lib/src redirection once"
			},
			{
				"command": "lib-file-corrector.enable",
				"title": "Enable lib/src redirection"
			},
			{
				"command": "lib-file-corrector.disable",
				"title": "Disable lib/src redirection"
			}
		]
	},
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.1.4",
		"vscode": "^1.1.25",
		"tslint": "^5.8.0",
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42"
	}
}
